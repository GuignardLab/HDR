	pages = {563--565},
	title = {{CANALIZATION} {OF} {DEVELOPMENT} {AND} {THE} {INHERITANCE} {OF} {ACQUIRED} {CHARACTERS}},
	urldate = {2019-11-18},
	volume = {150},
